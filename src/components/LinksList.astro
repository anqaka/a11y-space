---
interface Props {
  heading: string;
  list: {label: string, url: string}[]
}

const { heading, list } = Astro.props
---
<h4 class="custom-after mb-8 text-4xl font-bold text-primary">
  {heading}
</h4>
<ul class="list-disc pl-8 mb-8">
  {
    list.map((item) => (
      <li>
        <a href={item.url} target="_blank" class="">{item.label}</a>
      </li>
    ))
  }
</ul>
